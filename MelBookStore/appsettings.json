{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  // Tells the program where to go to find data
  "ConnectionStrings": {
    // Give it a name as a connection
    // IT NEEDS TO BE ALL ONE CONTINUOUS LINE (w/ no spaces) 
    // MSSQLLocalDB is the server 
    // Name the database that will actually be built 
    // Multiple batches can be connected 
    "StoreConnection": "Server=(localdb)\\MSSQLLocalDB;Database=StoreDB;MultipleActiveResultSets=true;"
  }
  }

